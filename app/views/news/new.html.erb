<% provide(:title, 'New News') %>
<h1>Create new news entry</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@news) do |f| %>
      <%= render 'form', f: f %>

      <%= f.submit "Create new news", class: "btn btn-large btn-primary" %>
    <% end %>

    <%= link_to 'Show', @news %> |
    <%= link_to 'Back', news_index_path %>

  </div>
</div>
