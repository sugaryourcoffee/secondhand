<% provide(:title, t('.New_News')) %>
<h1><%= t('.Create_news') %></h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@news.with_translations) do |f| %>
      <%= render 'form', f: f %>

      <p>
        <%= f.submit t('.Create_new_news'), class: "btn btn-large btn-primary" %>
      </p>
    <% end %>

    <p>
      <%= link_to t('.Show'), @news %> |
      <%= link_to t('.Back'), news_index_path %>
    </p>

  </div>
</div>
